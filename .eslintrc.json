{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "xo-typescript"],
	"overrides": [
		{
			"extends": ["xo-typescript", "plugin:@typescript-eslint/recommended"],
			"files": ["src/*.ts", "src/*.tsx"]
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"@typescript-eslint/no-unsafe-assignment": 0
	}
}
